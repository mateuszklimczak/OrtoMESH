<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" BackgroundColor="White" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="OrtoMESH.DoctorPage">
	<StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">

		<Button
			Text="Add healing process"
			TextColor="Black"
			FontSize="20"
			Clicked="AddProcessButton_Clicked"/>
		<Button
			Text="Search patient"
			TextColor="Black"
			FontSize="20"
			Clicked="SearchPatientButton_Clicked"
			/>
		
		<Button 
			Text="Back" 
			TextColor="Black" 
			Clicked="Back_Clicked"
			Margin="0,50,0,0"/>
	</StackLayout>
</ContentPage>
