<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" BackgroundColor="White" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="OrtoMESH.Doctor">
	<StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
		<Entry Text="Login" />
		<Entry Text="Password" />
		<Button Text="Sign In" TextColor="Black" WidthRequest="240" Clicked="SignIn_Clicked"/>
		<Button Text="Sign Up" TextColor="Black" WidthRequest="240" Clicked="SignUp_Clicked"/>
		<Button Text="Back" TextColor="Black" WidthRequest="240" Clicked="BackButton_Clicked" />
	</StackLayout>
</ContentPage>
