<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" BackgroundColor="White" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="OrtoMESH.RegistrationPagePatient">
	<StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
		<Entry x:Name="RegistrationNameEntry" Placeholder="Name" TextColor="Black" />
		<Entry x:Name="RegistrationSurnameEntry" Placeholder="Surname" TextColor="Black" />
		<Entry x:Name="RegistrationCityEntry" Placeholder="City" TextColor="Black" />
		<Entry x:Name="RegistrationAgeEntry" Placeholder="Age" TextColor="Black" />
		<Entry x:Name="RegistrationNumberEntry" Placeholder="Phone number" TextColor="Black" />
		<Entry x:Name="RegistrationMailEntry" Placeholder="Mail" TextColor="Black"/>
		<Button Text="Sign Up" TextColor="Black" Clicked="SignUpButton_Clicked"/>
		<Button Text="Back" TextColor="Black" Clicked="BackButton_Clicked" VerticalOptions="EndAndExpand"/>
	</StackLayout>
</ContentPage>
